{
    "compilerOptions": {
        "checkJs": false,
        "allowJs": true,
        "jsx": "react-jsx",
        "target": "ESNext",
        "noEmit": true,
        "module": "NodeNext",
        "outDir": "./public",
        "skipLibCheck": true,
        "moduleResolution": "nodenext",
        "esModuleInterop": true,
        "noEmitOnError": false ,
    },
    "include": [ "src/**/*" ]
}